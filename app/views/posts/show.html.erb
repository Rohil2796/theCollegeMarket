<p id="notice"><%= notice %></p>
<p>
  <strong>Name:</strong>
  <%= @post.name %>
  <%= @post.description %>
</p>
<% @attached_photos.each do |file| %>
  <li>
   <%= image_tag(file.asset.url(:thumb)), file.asset.url %>
   <%= link_to file.asset_file_name, file.asset.url, html_options = {download: 'Download Photo'} %>
  </li>
<% end %>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>