<%= simple_form_for @user do |f| %>
<%= render 'shared/error_messages', object: f.object %>
  <%= f.input :firstName %>
  <%= f.input :lastName %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :email %>
  <%= f.association :community, Community.order(:name),:name, :id, prompt: "Choose your Community" %>
  <%= f.button :submit %>
<% end %>