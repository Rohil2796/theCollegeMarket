<%= simple_form_for @user do |f| %>
<%= render 'shared/error_messages', object: f.object %>
  <%= f.input :firstName %>
  <%= f.input :lastName %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :email, label: "Enter in your community-specific email", prompt: "For Northwestern, use your 'u.northwestern.edu' email" %>
  <%= f.association :community %>
  <%= f.button :submit %>
<% end %>